<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" styleClass="background"
	stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.40"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="cbnu.inform.db.controller.MainOverviewController">
	<children>
		<TabPane minHeight="-Infinity" minWidth="-Infinity"
			prefHeight="400.0" prefWidth="600.0" styleClass="table-view"
			stylesheets="@DarkTheme.css" tabClosingPolicy="UNAVAILABLE"
			tabMaxHeight="400.0" tabMaxWidth="600.0" tabMinWidth="100.0"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<tabs>
				<Tab fx:id="registerTab" onSelectionChanged="#clickRegisterTab"
					text="  등 록  ">
					<content>
						<AnchorPane />
					</content>
				</Tab>
				<Tab fx:id="deleteTab" onSelectionChanged="#clickDeleteTab"
					text="  삭 제  ">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
							prefWidth="200.0" />
					</content>
				</Tab>
				<Tab fx:id="searchTab" onSelectionChanged="#clickSearchLayout"
					text="  검  색  ">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
							prefWidth="200.0" />
					</content>
				</Tab>
				<Tab fx:id="infoTab" onSelectionChanged="#clickInfoLayout" text="  수강 정보  ">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
							prefWidth="200.0" />
					</content>
				</Tab>
			</tabs>
		</TabPane>
	</children>
</AnchorPane>
